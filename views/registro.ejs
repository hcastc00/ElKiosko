<body>
    <nav class="navbar navbar-light navbar-expand-md navigation-clean-button">
        <div class="container">
            <div><a class="navbar-brand d-table-row" href="/" style="font-size: 40px;">El Kiosko</a><label class="form-label d-table-row" style="font-size: 1.5em;">Registro</label></div><button data-bs-toggle="collapse" class="navbar-toggler" data-bs-target="#navcol-1"><span class="visually-hidden">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
            <div class="collapse navbar-collapse" id="navcol-1">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item"></li>
                    <li class="nav-item"></li>
                </ul><span class="navbar-text actions"> <a class="login" href="/#loginForm" style="font-size: 20px;color: rgb(70, 87, 101);">Iniciar sesión</a><a class="btn btn-light action-button" role="button" href="/registro" style="font-size: 20px;background: rgb(179,236,236);color: rgb(34,34,34);">Regístrate!</a></span>
            </div>
        </div>
    </nav>
    <section class="register-photo" style="border-color: rgb(238,244,247);background: rgb(238,244,247);height: 821;">
        <div class="form-container" style="margin-top: 40px;">
            <div class="image-holder" style="background: url(&quot;assets/img/joinUs.png&quot;) left / cover no-repeat;"></div>
            <form method="post" action="afterRegister.html" style="box-shadow: inset 0px 0px;">
                <h2 class="text-center"><strong>Crea una cuenta</strong></h2><input class="form-control" type="text" id="usuario_registro" style="margin-bottom: 10px;" placeholder="Usuario">
                <div class="form-group mb-3"><input class="form-control" type="password" id="contrasenya_registro" name="password" placeholder="Contraseña"></div>
                <div class="form-group mb-3"><input class="form-control" type="password" id="repetir_contrasenya_registro" name="password-repeat" placeholder="Repetir contraseña"></div>
                <div class="btn-group btn-group-toggle" data-bs-toggle="tipoCuenta" style="border: 0;"><label class="form-label btn btn-outline-primary active" style="background-color: rgb(179,236,236);color: rgb(34,34,34);border: 0;">Socio&nbsp;<input type="radio" id="socio" name="btnradio"></label><label class="form-label btn btn-outline-primary" style="background-color: rgb(179,236,236);color: rgb(34,34,34);border: 0;">Administrador&nbsp;<input type="radio" id="administrador" name="btnradio"></label></div>
                <div class="form-group mb-3">
                    <div class="form-check"><label class="form-check-label"><input class="form-check-input" type="checkbox" id="check_terminos_registro">Estoy de acuerdo con los términos y condiciones.</label></div>
                </div>
                <div class="form-group mb-3"><button class="btn btn-primary d-block w-100" type="button" style="color: rgb(34,34,34);background: rgb(179,236,236);" onclick="registrar()">Crear cuenta</button></div><a class="already" href="/#loginForm">¿Ya tienes una cuenta? Inicia sesión aquí</a>
            </form>
        </div>
    </section>
    <script src="assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/js/bs-init.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="/assets/js/jquery.toast.js"></script>
    <script src="assets/js/registro.js"></script>
</body>

