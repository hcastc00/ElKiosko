                <div class="text-center form-group mb-3" style="height: 163px;"><img class="img-fluid" style="height: 100%;" src="/assets/img/authentication.png"></div>
                <div class="d-flex justify-content-center form-group mb-3"><input class="form-control" type="text" id="usuario_login" style="width: 200px;" placeholder="Usuario"></div>
                <div class="d-flex justify-content-center form-group mb-3"><input class="form-control" type="password" id="contrasenya_login" style="width: 200px;" placeholder="Contraseña"></div>
                <div class="d-flex justify-content-center form-group mb-3"><button class="btn btn-dark action-button" id="boton_submit_login" type="button" style="font-size: 20px;background: rgb(179,236,236);color: rgb(34,34,34);border-style: none;" onclick="login()">Iniciar Sesión</button></div>
            </form>
        </div>
